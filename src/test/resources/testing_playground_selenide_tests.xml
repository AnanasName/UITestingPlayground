    </test>

    <test name = "Scrollbars Test">
        <classes>
            <class name = "scrollbars_tests.scrollbars_selenide_test.ScrollbarsTest" />
        </classes>
    </test>

    <test name = "Test Input Test">
        <classes>
            <class name = "text_input_tests.text_input_selenide_test.TextInputTest" />
