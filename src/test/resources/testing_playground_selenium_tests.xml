<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Selenium Suite" thread-count="6" parallel="tests">

    <test name="Ajax Test">
        <classes>
            <class name="ajax_tests.ajax_selenium_test.AjaxDataTest"/>
        </classes>
    </test>

    <test name="Class Attributes Test">
        <classes>
            <class name="class_attr_tests.class_attr_selenium_test.ClassAttrTest"/>
        </classes>
    </test>

    <test name = "Class Button Test">
        <classes>
            <class name = "click_button_tests.click_button_selenium_test.ClickButtonTest" />
        </classes>
    </test>

    <test name = "Client Side Delay Test">
        <classes>
            <class name = "client_side_delay_tests.client_side_delay_selenium_test.ClientSideDelayTest" />
        </classes>
    </test>

    <test name = "Dynamic Id Test">
        <classes>
            <class name = "dynamic_id_tests.dynamic_id_selenium_test.DynamicIdTest" />
        </classes>
    </test>

    <test name = "Mouse Over Test">
        <classes>
            <class name = "mouse_over_tests.mouse_over_selenium_test.MouseOverTest" />
        </classes>
    </test>

    <test name = "Load Delay Test">
        <classes>
            <class name = "load_delay_tests.load_delay_selenium_test.LoadDelayTest" />
        </classes>
    </test>

    <test name = "Non Breaking Space Test">
        <classes>
            <class name = "non_breaking_tests.non_breaking_selenium_test.NonBreakingSpaceTest" />
        </classes>
    </test>

    <test name = "Progress Bar Test">
        <classes>
            <class name = "progress_bar_tests.progress_bar_selenium_test.ProgressBarTest" />
        </classes>
    </test>

    <test name = "Sample App Test">
        <classes>
            <class name = "sample_app_tests.sample_app_selenium_test.page.SampleAppPage" />
        </classes>
    </test>

    <test name = "Scrollbars Test">
        <classes>
            <class name = "scrollbars_tests.scrollbars_selenium_test.ScrollbarsTest" />
        </classes>
    </test>

    <test name = "Test Input Test">
        <classes>
            <class name = "text_input_tests.text_input_selenium_test.TextInputTest" />
        </classes>
    </test>

</suite>